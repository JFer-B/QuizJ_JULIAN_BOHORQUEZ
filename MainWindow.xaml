        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EventParadigmExample"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid Background="Black">
        <Label Content="King-Dumb-" HorizontalAlignment="Center" Margin="0,20,0,0" VerticalAlignment="Top" FontSize="40" FontFamily="Ink Free" Background="{x:Null}" FontWeight="Bold" Foreground="White"/>
        <Frame Name="frameMain" Content="" HorizontalAlignment="Center" Height="300" Margin="0,85,0,0" VerticalAlignment="Top" Width="700" NavigationUIVisibility="Hidden"/>
        
    </Grid>
</Window>
